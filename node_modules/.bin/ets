#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../_egg-bin@4.15.0@egg-bin/bin/ets.js" "$@"
  ret=$?
else 
  node  "$basedir/../_egg-bin@4.15.0@egg-bin/bin/ets.js" "$@"
  ret=$?
fi
exit $ret
